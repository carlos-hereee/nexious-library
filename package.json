{
  "name": "nexious-library",
  "version": "1.2.1",
  "description": "Official database for nexious.tech",
  "type": "module",
  "main": "dist/esm/main.js",
  "style": "dist/css/index.css",
  "homepage": "https://github.com/carlos-hereee/nexious-library#readme",
  "author": "carlos-hereee <97hernandez.c@gmail.com>(https://www.nexious.tech)",
  "license": "MIT",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/main.d.ts",
        "default": "./dist/esm/main.js"
      },
      "require": {
        "types": "./dist/cjs/main.d.ts",
        "default": "./dist/cjs/main.js"
      }
    },
    "./@nxs-atoms": {
      "import": {
        "types": "./dist/esm/atoms/index.d.ts",
        "default": "./dist/esm/atoms/index.js"
      },
      "require": {
        "types": "./dist/cjs/atoms/index.d.ts",
        "default": "./dist/cjs/atoms/index.cjs"
      }
    },
    "./@nxs-molecules": {
      "import": {
        "types": "./dist/esm/molecules/index.d.ts",
        "default": "./dist/esm/molecules/index.js"
      },
      "require": {
        "types": "./dist/cjs/molecules/index.d.ts",
        "default": "./dist/cjs/molecules/index.cjs"
      }
    },
    "./@nxs-organism": {
      "import": {
        "types": "./dist/esm/organism/index.d.ts",
        "default": "./dist/esm/organism/index.js"
      },
      "require": {
        "types": "./dist/cjs/organism/index.d.ts",
        "default": "./dist/cjs/organism/index.cjs"
      }
    },
    "./@nxs-template": {
      "import": {
        "types": "./dist/esm/template/index.d.ts",
        "default": "./dist/esm/template/index.js"
      },
      "require": {
        "types": "./dist/cjs/template/index.d.ts",
        "default": "./dist/cjs/template/index.cjs"
      }
    },
    "./@nxs-math": {
      "import": {
        "types": "./dist/esm/math/index.d.ts",
        "default": "./dist/esm/math/index.js"
      },
      "require": {
        "types": "./dist/cjs/math/index.d.ts",
        "default": "./dist/cjs/math/index.cjs"
      }
    },
    "./@index.css": "./dist/css/index.css"
  },
  "_moduleAliases": {
    "@nxs-atoms": "dist/esm/atoms",
    "@nxs-molecules": "dist/esm/molecules",
    "@nxs-organism": "dist/esm/organism",
    "@nxs-template": "dist/esm/template",
    "@nxs-math": "dist/esm/math",
    "@nxs-helpers": "dist/esm/helpers"
  },
  "scripts": {
    "nodemon": "nodemon",
    "dev:ts-node": "ts-node -r tsconfig-paths/register -r src/main.ts",
    "dev:webpack": "webpack-dev-server --stats-error-details",
    "watch:1": "npx sass src/stylesheets:dist/css --watch",
    "watch:2": "tsc -p tsconfig.json --watch",
    "watch:3": "tsc-alias -p tsconfig.json --watch",
    "copy:esm": "copyfiles -u 1 src/**/*.html src/**/*.css src/**/*.scss dist/esm",
    "copy:css": "node ./copy-styles.js",
    "copy-styles:esm": "node --loader ts-node/esm .copy-styles.ts",
    "clean": "rimraf dist/ && npx prettier --write src/",
    "compile": "npm run compile:tsc && npm run compile:tsc-alias && npm run compile:css",
    "compile:tsc": "tsc -p tsconfig.json",
    "compile:js": "tsc --module commonjs --outDir dist/cjs",
    "compile:tsc-alias": "tsc-alias -p tsconfig.json dist/esm",
    "compile:css": "npx sass src/stylesheets:dist/css",
    "compile:paths": "node -r tsconfig-paths/register dist/esm/main.js",
    "compile:webpack": "webpack --mode production --stats-error-details",
    "build": "npm run clean && npm run compile",
    "build:tsup": "npm run clean && npm run compile:tsup && npm run sass",
    "build:webpack": "npm run clean && npm run compile && npm run",
    "build:serve": "webpack serve --stats-error-details",
    "build:alias": "npm run clean && npm run compile:alias && npm run sass",
    "release:patch": "npm run build && npm version patch && npm publish",
    "release:minor": "npm run build && npm version minor && npm publish",
    "release:major": "npm run build && npm version major && npm publish"
  },
  "keywords": [
    "css",
    "sass",
    "mobile-first",
    "responsive",
    "front-end",
    "react",
    "react-vite"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carlos-hereee/nexious-library.git"
  },
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/carlos-hereee/nexious-library/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@types/jest": "^29.5.2",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "2.8.8",
    "rimraf": "^5.0.1",
    "sass": "^1.62.1",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.6",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "react-router-dom": "^6.14.0"
  }
}
