{
  "name": "nexious-library",
  "version": "1.1.12",
  "description": "Official database for nexious.tech",
  "type": "module",
  "module": "dist/esm/main.js",
  "types": "src/esm/main.d.ts",
  "sass": "src/stylesheets/index.scss",
  "style": "dist/index.css",
  "homepage": "https://github.com/carlos-hereee/nexious-library#readme",
  "author": "carlos-hereee <97hernandez.c@gmail.com>(https://www.nexious.tech)",
  "license": "MIT",
  "exports": {
    ".": "./dist/esm/main.js",
    "./math": "./dist/esm/math/index.js",
    "./atoms": "./dist/esm/atoms/index.js",
    "./molecules": "./dist/esm/molecules/index.js",
    "./organism": "./dist/esm/organism/index.js",
    "./index.css": "./dist/esm/stylesheets/index.css"
  },
  "_moduleAliases": {
    "~": "src",
    "atoms": "src/atoms/*",
    "molecules": "src/molecules/*",
    "organism": "src/organism/*",
    "math": "src/math/*",
    "helpers": "src/helpers/*"
  },
  "keywords": [
    "css",
    "sass",
    "mobile-first",
    "responsive",
    "front-end",
    "react",
    "react-vite"
  ],
  "scripts": {
    "watch": "tsc -p tsconfig.json --watch",
    "dev": "npm run build && npm run watch",
    "dev:ts-node": "ts-node -r tsconfig-paths/register -r src/main.ts",
    "dev:webpack": "webpack-dev-server --stats-error-details",
    "sass": "npx sass src/stylesheets && npm run copy:esm && npm run copy:esm",
    "sass:watch": "npx sass --watch src/stylesheets",
    "copy:esm": "copyfiles -u 1 src/**/*.html src/**/*.css src/**/*.scss dist/esm",
    "copy-styles:esm": "node --loader ts-node/esm .copy-styles.ts",
    "prettier": "npx prettier --write src/",
    "clean": "rimraf dist/ && npm run prettier",
    "paths": "node -r tsconfig-paths/register -r ts-node ./src/main.ts",
    "compile": "tsc && tsc --module commonjs --outDir dist/cjs",
    "compile:alias": "tsc && tsc-alias && npm run paths",
    "build": "npm run clean && npm run compile && npm run sass",
    "build:tsup": "npm run clean && npm run compile:tsup && npm run sass",
    "build:webpack": "webpack --mode production --stats-error-details",
    "build:serve": "webpack serve --stats-error-details",
    "build:mac": "npm run clean:mac && npm run sass:build",
    "build:tsc": "node -r tsconfig-paths/register ts-node ./src/main.ts",
    "build:alias": "npm run clean && npm run compile:alias",
    "start": "npm run build && npm link",
    "release:patch": "npm unlink && npm run build && npm version patch && npm publish",
    "release:minor": "npm unlink && npm run build && npm version minor && npm publish",
    "release:major": "npm unlink && npm run build && npm version major && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carlos-hereee/nexious-library.git"
  },
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/carlos-hereee/nexious-library/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@types/jest": "^29.5.2",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "babel-loader": "^9.1.2",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.8.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.2.1",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "module-alias": "^2.2.3",
    "nodemon": "^2.0.22",
    "prettier": "2.8.8",
    "rimraf": "^5.0.1",
    "sass": "^1.62.1",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.6",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "webpack": "^5.86.0",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "peerDependencies": {
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0"
  }
}